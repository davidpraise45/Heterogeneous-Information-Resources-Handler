<%- include('../header') %>
<br>
    <form method="post" action="/addPopulatedPlace">
        <div class="form-group">
            <label for="stateId">State ID</label>
            <input type="text" class="form-control" id="stateId" name="stateId" required>
        </div>
        <div class="form-group">
            <label for="populatedPlaceID">Place ID</label>
            <input type="text" class="form-control" id="id" name="id" value="<%=populatedPlaces.length+1%>" required>
        </div>
        <div class="form-group">
            <label for="name">Name</label>
            <input type="text" class="form-control" id="name" name="name" required>
        </div>
        <div class="form-group">
            <label for="pttCode">PTT Code</label>
            <input type="text" class="form-control" id="pttCode" name="pttCode">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
<br>
<%- include('../footer') %>
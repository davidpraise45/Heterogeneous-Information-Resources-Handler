<%- include('../header') %>

    <form method="post" action="/editStateById/<%= state.DR_IDENTIFIKATOR %>">
        <div class="form-group">
            <label>State ID</label>
            <input type="text" class="form-control" id="stateId" name="stateId" value="<%= state.DR_IDENTIFIKATOR %>">
        </div>
        <div class="form-group">
            <label>State name</label>
            <input type="text" class="form-control" id="stateName" name="stateName" value="<%= state.DR_NAZIV %>">
        </div>
        <div class="form-group">
            <label>Foundation Date</label>
            <input type="text" class="form-control" id="stateFoundationDate" name="stateFoundationDate" value="<%= state.DR_DATUM_OSNIVANJA %>">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>

<%- include('../footer') %>